{
    "multipart": [
        { "when": {"facing": "east","triggered": false }, "apply": { "y": 90, "model": "bubbles:block/bellows_unpushed" } },
        { "when": {"facing": "west","triggered": false }, "apply": { "y": 270, "model": "bubbles:block/bellows_unpushed" } },
        { "when": {"facing": "south","triggered": false }, "apply": { "y": 180, "model": "bubbles:block/bellows_unpushed" } },
        { "when": {"facing": "north","triggered": false }, "apply": { "y": 0, "model": "bubbles:block/bellows_unpushed" } },
        { "when": {"facing": "east","triggered": true }, "apply": { "y": 90, "model": "bubbles:block/bellows_pushed" } },
        { "when": {"facing": "west","triggered": true }, "apply": { "y": 270, "model": "bubbles:block/bellows_pushed" } },
        { "when": {"facing": "south","triggered": true }, "apply": { "y": 180, "model": "bubbles:block/bellows_pushed" } },
        { "when": {"facing": "north","triggered": true }, "apply": { "y": 0, "model": "bubbles:block/bellows_pushed" } },
        { "when": {"facing": "east","triggered": "true|false" }, "apply": { "y": 90, "model": "bubbles:block/bellows_base" } },
        { "when": {"facing": "west","triggered": "true|false" }, "apply": { "y": 270, "model": "bubbles:block/bellows_base" } },
        { "when": {"facing": "south","triggered": "true|false" }, "apply": { "y": 180, "model": "bubbles:block/bellows_base" } },
        { "when": {"facing": "north","triggered": "true|false" }, "apply": { "y": 0, "model": "bubbles:block/bellows_base" } }
    ]
}